<div class="row">
  <div class="col-sm-5">
    <h3>My User Profile | <%= link_to "Edit Profile", edit_user_registration_path %></h3>
    <br>
    <h3><%= current_user.email %></h3>
    <%= image_tag current_user.avatar.url(:medium) %>
  </div>

  <div class="col-sm-7">
    <h3>My Classifieds</h3>
  </div>
</div>

<div class="row">
  <div class="col-sm-12">
  <h4>Account Details</h4>
  <ul>
    <li>Account created: <%= current_user.created_at %></li>
    <li>Last login: <%= current_user.last_sign_in_at %></li>
  </ul>

  </div>
</div>
